        <ExtendedData>
          <Data name="gx_media_links">
            <value>https://lh3.googleusercontent.com/proxy/-mrQuUgIdGr_qEHpMROgk4eh7wGiDpWqdNVP9dsR3BE8GLHYfI_app3x05jVR33d3QBG7D0-iZQ3Qu16pe2m6gu0RUR91XKxUkIis8AF9V5ljsW7qlxfblMFMEgpQIKCUJiDdHuDihVpDV_aLmcAxnfsgjA4OGUgp1o0ZFoW5eSZG80QYpALjypS1KiB2XCizw90pA4ok8l_f1UoyBOnqkj6kB3bdcUCLRgDwMM6esRlGjtZCERDrQMuSLlwyfNHcTvBq52_ZnKf-946SffANl26xgTtYmp1G-5cUA</value>
