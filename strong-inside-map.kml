          <Data name="gx_media_links">
            <value>https://lh3.googleusercontent.com/proxy/2EmM4Qaj3dAgD_K3KGcBy8ps-qnpBEBCyjuWGdrhoCnbtEJkPogVznwXAU7ODzP8Fp_bgKF2b75zwdaOlk75FdAY0rEaZureB-dT17k9VhX080Gshy1rCJkfUb0QfWGiV-rbwXDGwiqUgFk2YySPI-vdDeY2GGxX0mUIUWoS95oz-OZqfpXocLqjbmADxuHDpNlWIkp8by4yRyRLUwwTAr-pHZPRecFVJ_AmcIBewWRaiALHfz_AClfCih8XFwEE_ytdQyyO1QORSy3Cf3d3ZnaFOI8OEm0c1PHOhSbKEVuvLVCzNu86telBpkZKuybDJ1EhFn88PgphOgIOlU3XA7zGrQ</value>
